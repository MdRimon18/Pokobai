    <script>
       window.showSuccessToast = function (message = "Your action was successful!") {
        var successToast = document.getElementById('successToast');
        successToast.classList.remove('d-none'); // Remove the d-none class to show the toast
        document.querySelector('#successToast .toast-body').textContent = message;
        var toastElement = new bootstrap.Toast(successToast);
        toastElement.show();
       };

        function showDangerToast(message = "Something went wrong!") {
            var dangerToast = document.getElementById('dangerToast');
            dangerToast.classList.remove('d-none'); // Remove the d-none class to show the toast
            document.querySelector('#dangerToast .toast-body').textContent = message;
            var toastElement = new bootstrap.Toast(dangerToast);
            toastElement.show();
        }

        function showWarningToast(message = "Be cautious while performing this action.") {
            var warningToast = document.getElementById('warningToast');
            warningToast.classList.remove('d-none'); // Remove the d-none class to show the toast
            document.querySelector('#warningToast .toast-body').textContent = message;
            var toastElement = new bootstrap.Toast(warningToast);
            toastElement.show();
        }

    </script>

    <script>
        window.showToast = function (type = "success", message = "Your action was successful!") {
            var toastContainer = document.getElementById("globalToast");

            // Set toast type classes dynamically
            toastContainer.className = `toast align-items-center text-white d-none bg-${type}`;

            // Change text color for warning (better visibility)
            if (type === "warning") {
                toastContainer.classList.add("text-dark");
            }

            // Set the message
            document.querySelector("#globalToast .toast-body").textContent = message;

            // Show the toast
            toastContainer.classList.remove("d-none");
            var toast = new bootstrap.Toast(toastContainer);
            toast.show();
        };
    </script>
   


